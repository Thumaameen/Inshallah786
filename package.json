{
  "name": "dha-digital-services",
  "version": "2.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.19.1",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "start": "NODE_ENV=production node --experimental-modules --es-module-specifier-resolution=node dist/server/index-minimal.js",
    "dev": "npx tsx server/index-minimal.ts",
    "replit:dev": "NODE_ENV=development npx tsx --enable-source-maps server/index-minimal.ts",
    "render-start": "node dist/server/index-minimal.js",
    "build": "npm run clean && npm run setup:deps && npm run build:all",
    "setup:deps": "npm install && cd client && npm install --legacy-peer-deps && npm install --save-dev vite@latest @vitejs/plugin-react typescript @types/react @types/react-dom @types/node",
    "build:all": "npm run build:server && npm run build:client && npm run copy:client",
    "build:client": "cd client && npm run build || true",
    "build:server": "tsc -p tsconfig.production.json",
    "copy:client": "mkdir -p dist/public && cp -r client/dist/* dist/public/ || echo 'No client files to copy'",
    "clean": "rm -rf dist client/dist node_modules/.cache",
    "setup:types": "npm install --save-dev @types/node @types/express",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "worker": "node server/workers/monitoring-worker.cjs",
    "cron": "node server/workers/scheduled-tasks.cjs",
    "validate": "node ai-validation-suite.cjs",
    "test:all": "npx tsx comprehensive-system-test.ts",
    "test:render": "npx tsx test-render-deployment.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.67.0",
    "@esbuild/linux-x64": "^0.25.11",
    "@radix-ui/react-slot": "^1.2.3",
    "@solana/web3.js": "^1.98.4",
    "@types/better-sqlite3": "^7.6.8",
    "@types/express-session": "^1.17.7",
    "@types/multer": "^1.4.7",
    "@types/node-cron": "^3.0.8",
    "@types/node-forge": "^1.3.5",
    "@types/pg": "^8.10.0",
    "@types/qrcode": "^1.5.2",
    "@types/socket.io": "^3.0.2",
    "ajv": "^8.17.1",
    "axios": "^1.6.0",
    "bcryptjs": "^3.0.2",
    "better-sqlite3": "^9.0.0",
    "canvas": "^3.2.0",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "drizzle-orm": "^0.29.0",
    "esbuild": "^0.21.5",
    "ethers": "^6.15.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.2",
    "helmet": "^7.1.0",
    "jsbarcode": "^3.12.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.552.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "node-forge": "^1.3.1",
    "openai": "^4.0.0",
    "pdf-lib": "^1.17.1",
    "pdfkit": "^0.14.0",
    "postgres": "^3.4.7",
    "qrcode": "^1.5.4",
    "rate-limiter-flexible": "^8.1.0",
    "sharp": "^0.34.4",
    "socket.io": "^4.8.1",
    "tsx": "^4.19.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^5.0.5",
    "@types/helmet": "^4.0.0",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/web3": "^1.2.2",
    "@types/ws": "^8.18.1",
    "@tanstack/react-query": "^5.28.0",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "drizzle-kit": "^0.31.6",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "web3": "^4.16.0"
  }
}
