{
  "name": "dha-digital-services",
  "version": "2.0.0",
  "type": "module",
  "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "start": "NODE_ENV=production node dist/server/index-minimal.js",
    "dev": "npx tsx server/index-minimal.ts",
    "build": "npm run clean && npm run build:client && npm run build:server && npm run copy:client",
    "build:client": "cd client && npm install --legacy-peer-deps && npm run build",
    "build:server": "tsc -p tsconfig.production.json --skipLibCheck",
    "copy:client": "mkdir -p dist/public && cp -r client/dist/* dist/public/",
    "clean": "rm -rf dist client/dist",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "worker": "node server/workers/monitoring-worker.cjs",
    "cron": "node server/workers/scheduled-tasks.cjs",
    "validate": "node ai-validation-suite.cjs",
    "test:all": "npx tsx comprehensive-system-test.ts",
    "test:render": "npx tsx test-render-deployment.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.67.0",
